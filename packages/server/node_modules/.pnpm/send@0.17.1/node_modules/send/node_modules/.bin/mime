#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/cli.js/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/packages/server/node_modules/.pnpm/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/packages/server/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/packages/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/node_modules:/home/renato/Documents/Cursos/hirincoders/node_modules:/home/renato/Documents/Cursos/node_modules:/home/renato/Documents/node_modules:/home/renato/node_modules:/home/node_modules:/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/mime/cli.js/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/mime/node_modules:/home/renato/Documents/Cursos/hirincoders/semana16/client-server-example/packages/server/node_modules/.pnpm/send@0.17.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../mime/cli.js" "$@"
else 
  exec node  "$basedir/../../../mime/cli.js" "$@"
fi
